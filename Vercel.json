{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "devCommand": null,
  "routes": [
    {
      "src": "/ads.txt",
      "dest": "/public/ads.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}